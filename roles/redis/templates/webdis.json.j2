{
  "redis_host": "{{ redis_host }}",
  "redis_port": {{ redis_port }},
  "redis_auth": "{{ redis_auth }}",
  "http_host": [{{ webdis_http_host }}],
  "threads": {{ webdis_threads }},
  "pidfile": "/var/run/webdis/webdis.pid",
  "daemonize": true,
  "http_port": {{ webdis_http_port }},
  "https": true,
  "ssl_certificate": "{{ webdis_ssl_certificate }}",
  "ssl_private_key": "{{ webdis_ssl_private_key }}",
  "acl": [
      {
          "disabled":["DEBUG"]
      },

      {
          "http_basic_auth": "user:password",
          "enabled": ["DEBUG"]
      }
  ],
  "logfile": "/var/log/webdis/webdis.log"
}